{
  "version": 2,
  "routes": [
    {
      "src": "/admin",
      "dest": "/index.html"
    },
    {
      "src": "/admin/callback",
      "dest": "/index.html"
    },
    {
      "src": "/dictionary/word/(.*)",
      "dest": "/index.html"
    },
    {
      "src": "/(.*)\\.js",
      "headers": {
        "Content-Type": "application/javascript"
      },
      "dest": "/$1.js"
    },
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ]
} 